html{
    font-size: 16px;
}

body{
	padding: 0;
	margin: 0;

}

a{
    text-decoration: none;
}
ul{
    list-style-type: disc;
    margin-block-start: 0;
    margin-block-end: 0;
    margin-inline-start: 0px;
    margin-inline-end: 0px;
    padding-inline-start: 0;
}
p {
    display: block;
    margin-block-start: 0;
    margin-block-end: 0;
    margin-inline-start: 0;
    margin-inline-end: 0;
}



.container{
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin: 0 auto;
 } 

.nav_container{
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin: 0 auto;
    max-width: 1200px;
}

.nav_global {
    display: flex;
    justify-content: space-between;
    text-align: center;
    /* max-width: 1200px; */
    width: 100%;
    top: 0px;
    z-index: 5;
    margin: 0 auto;
    position: relative;
    height: 2rem;
    margin: 0 !important;
    border: 1px solid transparent;
    font-family:ReithSans,Helvetica,Arial,freesans,sans-serif;
    font-weight:700;
    font-size:0.8125rem;
    line-height:1rem;

    }
.nav_global ul {
    list-style: none;
}
    
.nav_global a {
    text-decoration: none;
}

.navbar_topmenu, .navbar_downmenu{
    display: flex;
    justify-content: center;
    /* align-content: center; */
    width: 100%;
    padding-inline-start: 0px;
    margin: 0 auto;
    height: 2rem;
}
  
  
.navbar_topmenu > li, .navbar_downmenu > li{
    position: relative;
    padding-top: 0.25rem;
    display: flex;
}

.navbar_topmenu > li img,  .navbar_downmenu > li img{
    margin: 0;
    padding: 0;
}
  
.navbar_topmenu > li > a{
    margin-top: 0;
    color: black;
    padding:0.25rem 1.875rem;
    border-bottom-width:0.25rem;
    border-bottom-color:#007BC7;
}

.navbar_downmenu > li > a{
    margin-top: 0;
    color: white;
    padding:0.25rem 1.875rem;
    border-bottom-width:0.25rem;
    border-bottom-color:white;
    font-family:ReithSans,Helvetica,Arial,freesans,sans-serif;
    font-weight:700;
    font-size:0.8125rem;
    line-height:1rem;

}

.navbar_topmenu > li > a:hover,  .navbar_downmenu > li >a:hover{
    padding-bottom: 0;
    border-bottom-style:solid;
}


.nav_local{
    background-color: #B80000;
    margin: 0;
    color: white;
}

.header_news{
    padding-top: 1.5rem;
    padding-bottom: 1.5rem;
    padding-left: 1.5rem;
    font-size: 5rem;
    font-family:sans-serif;
 }


 .—ÅontentWrapper{
    width:100%;
    display:flex;
    align-items:center;
    justify-content:space-between;
    z-index: -9999;
}

.visuallyHidden{
    position:absolute;
    overflow:hidden;
    margin:0;
    width:1px;
    height:1px;
    clip-path:inset(100%);
    clip:rect(1px,1px,1px,1px);
    white-space:nowrap;
}


.logoLink{
    display:flex;
    align-items:center;
    width:auto;
    text-decoration:none;
    color:inherit;
    margin-left: 1rem;
}

.logo{
    display:block;
    padding:0.75rem 0;
    height:1.5rem;
    fill:currentcolor;
    max-width:10rem;
}

hr{
    margin: 0;
}





























